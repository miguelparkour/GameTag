{
    "name": "games",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@brainhubeu/react-carousel": "^2.0.1",
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "@mui-treasury/components": "^1.9.1",
        "@mui-treasury/styles": "^1.13.0",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "react": "^16.13.1",
        "react-circle-progress-bar": "^0.1.4",
        "react-dom": "^16.13.1",
        "react-google-font-loader": "^1.1.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "3.4.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "prettier": "prettier --write ."
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "prettier": "^2.1.2"
    },
    "prettier": {
        "printWidth": 110,
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": false,
        "proseWrap": "always",
        "overrides": [
            {
                "files": "package.json",
                "options": {
                    "printWidth": 250
                }
            },
            {
                "files": ".ci.yml",
                "options": {
                    "printWidth": 150
                }
            },
            {
                "files": "*.md",
                "options": {
                    "tabWidth": 2
                }
            }
        ]
    }
